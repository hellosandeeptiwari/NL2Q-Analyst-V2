# AI/LLM Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-claude-key-here
GOOGLE_API_KEY=your-gemini-api-key-here
DEFAULT_LLM_PROVIDER=openai

# Database Connections
DATABASE_URL=postgresql://username:password@localhost:5432/nl2q_v2
SNOWFLAKE_ACCOUNT=your-snowflake-account
SNOWFLAKE_USER=your-snowflake-user
SNOWFLAKE_PASSWORD=your-snowflake-password
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=your-database
SNOWFLAKE_SCHEMA=PUBLIC
MONGODB_URI=mongodb://localhost:27017/nl2q_v2
BIGQUERY_PROJECT_ID=your-gcp-project
REDSHIFT_HOST=your-redshift-cluster.region.redshift.amazonaws.com
REDSHIFT_PORT=5439
REDSHIFT_USER=your-redshift-user
REDSHIFT_PASSWORD=your-redshift-password
REDSHIFT_DATABASE=your-redshift-db

# Authentication & Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=30
OAUTH_PROVIDERS=google,github,azure
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret
GITHUB_OAUTH_CLIENT_ID=your-github-oauth-client-id
GITHUB_OAUTH_CLIENT_SECRET=your-github-oauth-client-secret

# Infrastructure
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_TTL=3600
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_ENVIRONMENT=your-pinecone-environment
PINECONE_INDEX_NAME=nl2q-schema-v2

# API Configuration
API_V2_PREFIX=/api/v2
CORS_ORIGINS=http://localhost:3000,http://localhost:8080
MAX_QUERY_EXECUTION_TIME=300
MAX_CONCURRENT_QUERIES=10

# Feature Flags
ENABLE_ML_SUGGESTIONS=true
ENABLE_REAL_TIME_COLLAB=true
ENABLE_MULTI_TENANT=true
ENABLE_QUERY_CACHING=true
ENABLE_STREAMING_RESPONSES=true
ENABLE_ADVANCED_ANALYTICS=true

# Development
DEBUG=true
LOG_LEVEL=INFO
ENVIRONMENT=development

# Monitoring & Observability
SENTRY_DSN=your-sentry-dsn
ENABLE_METRICS=true
METRICS_PORT=8001

# Email & Notifications
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
FROM_EMAIL=noreply@nl2q-analyst-v2.com

# Storage
STORAGE_PROVIDER=local  # Options: local, s3, gcs, azure
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_BUCKET_NAME=your-s3-bucket
AWS_REGION=us-east-1

# ML/AI Configuration
ML_MODEL_PATH=./models
ENABLE_AUTO_ML=false
SUGGESTION_MODEL_VERSION=v2.1
ANALYTICS_MODEL_VERSION=v1.5

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# Security Headers
SECURITY_HEADERS_ENABLED=true
HTTPS_ONLY=false  # Set to true in production

# Multi-tenant Configuration
DEFAULT_TENANT_ID=default
MAX_TENANTS=100
TENANT_ISOLATION_MODE=strict  # Options: strict, relaxed